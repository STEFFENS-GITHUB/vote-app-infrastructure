[
    {
      "name": "vote",
      "image": "dockersamples/examplevotingapp_vote",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 5000,
          "hostPort": 5000,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/terraform-ecs-logs",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs",
          "awslogs-create-group": "true"
        }
      },
      "ephemeralStorage": {
        "sizeInGiB": 20
      },
      "memory": 2048,
      "cpu": 512
    }
  ]